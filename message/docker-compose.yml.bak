version: '3.7'

services:
    message:
        build: .
        restart: always
        ports:
          - 9002:9002
        depends_on:
          - postgres_db
    postgres_db:
        image: "postgres"
        restart: always
        ports:
          - 5433:5432
        environment:
            POSTGRES_DB: uqacter
            POSTGRES_PASSWORD: postgres

